@using Microsoft.AspNetCore.Authentication

<h2>Claims For MvcClient1</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    
    @{
        var aa = await Context.AuthenticateAsync();
    }
    @foreach (var prop in aa.Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>